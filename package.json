{
    "name": "@atlasconsulting/bedita-sdk",
    "version": "1.1.0",
    "description": "A simple BEdita SDK based on axios",
    "repository": {
        "type": "git",
        "url": "https://github.com/atlasconsulting/bedita-sdk-js.git"
    },
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "./node_modules/.bin/tsc",
        "test": "TS_NODE_COMPILER_OPTIONS='{ \"module\": \"commonjs\" }' mocha -r ts-node/register --require mock-local-storage --require tests/hooks.ts tests/**/*.test.ts",
        "coverage": "nyc yarn run test",
        "lint": "eslint . --ext .ts"
    },
    "author": "Atlas <dev@atlasconsulting.it> (https://atlasconsulting.it)",
    "license": "MIT",
    "publishConfig": {
        "access": "public"
    },
    "dependencies": {
        "axios": "^0.25.0",
        "tslib": "^2.3.1"
    },
    "devDependencies": {
        "@istanbuljs/nyc-config-typescript": "^1.0.2",
        "@types/chai": "^4.3.0",
        "@types/mocha": "^9.1.0",
        "@types/node": "^17.0.13",
        "@typescript-eslint/eslint-plugin": "^5.10.1",
        "@typescript-eslint/parser": "^5.10.1",
        "chai": "^4.3.6",
        "eslint": "^8.8.0",
        "mocha": "^9.2.0",
        "mock-local-storage": "^1.1.19",
        "nyc": "^15.1.0",
        "source-map-support": "^0.5.21",
        "ts-node": "^10.4.0",
        "typescript": "^4.5.5"
    }
