{
    "name": "@atlasconsulting/bedita-sdk",
    "version": "1.0.0",
    "description": "A simple BEdita SDK based on axios",
    "repository": {
        "type": "git",
        "url": "https://github.com/atlasconsulting/bedita-sdk-js.git"
    },
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "./node_modules/.bin/tsc",
        "test": "TS_NODE_COMPILER_OPTIONS='{ \"module\": \"commonjs\" }' mocha -r ts-node/register --require mock-local-storage --require tests/hooks.ts tests/**/*.test.ts",
        "coverage": "nyc yarn run test",
        "lint": "tslint -c tslint.json 'src/**/*{.ts,.tsx}'"
    },
    "author": "Atlas <dev@atlasconsulting.it> (https://atlasconsulting.it)",
    "license": "MIT",
    "publishConfig": {
        "access": "public"
    },
    "dependencies": {
        "axios": "^0.21.2",
        "tslib": "^1.11.1"
    },
    "devDependencies": {
        "@istanbuljs/nyc-config-typescript": "^1.0.1",
        "@types/chai": "^4.3.0",
        "@types/mocha": "^9.0.0",
        "@types/node": "^17.0.4",
        "chai": "^4.3.4",
        "mocha": "^9.1.3",
        "mock-local-storage": "^1.1.11",
        "nyc": "^15.0.1",
        "source-map-support": "^0.5.16",
        "ts-node": "^10.4.0",
        "tslint": "^6.1.1",
        "typescript": "^4.5.5"
    }
}
